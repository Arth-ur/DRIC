<section class="content body page" id="page-devtools" hidden>
    <section class="content-header">
        <h1>
            Developer tools
        </h1>
    </section>
    <section class="content">
        <div class="row">
            <div class="col-md-6">
                <div class="box box-default">
                    <div class="box-header with-border">
                        <h3 class="box-title">Loaded plugins</h3>
                    </div>
                    <div class="box-body">
                        <table class="table">
                            <thead><tr><th>Name</th></tr></thead>
                            <tbody>
                                {{#plugins}}
                                <tr><td>{{name}}</td></tr>
                                {{/plugins}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>
</section>