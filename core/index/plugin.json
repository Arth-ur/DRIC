{
    "manifest-version": 1.0,
    "name": "DRIC index page provider",
    "id": "DRICindex",
    "description": "This plugin provides the index page",
    "author": "Arthur Gay",
    "load": ["index.py"],
    "routes": {
        "/": "index"
    },
    "static_dirs": {
        "/css/": "css",
        "/img/": "img",
        "/js/": "js",
        "/templates/": "templates",
        "/plugins/": "plugins"
    }
}
